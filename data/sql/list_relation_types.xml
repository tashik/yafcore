<?xml version="1.0" encoding="UTF-8"?>
<table name="list_relation_types">
  <comment>Список типов взаимосвязей</comment>
  <columns>
    <column name="id">
      <type>serial</type>
      <constraint>not-null primary</constraint>
    </column>
    <column name="name">
      <type length="99">varchar</type>
    </column>
    <column name="description">
      <type length="255">varchar</type>
    </column>
  </columns>
</table>
